{
  "name": "blog",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --colors",
    "lint": "eslint ./src --ignore-path .gitignore",
    "production": "webpack -p",
    "serve": "webpack-dev-server",
    "start": "concurrently \"yarn stubby\" \"yarn serve\"",
    "stubby": "stubby -w -d test/stubs/stubby.json",
    "test": "concurrently \"yarn lint\" \"yarn test:unit\"",
    "test:unit": "nyc mocha test/unit --compilers js:babel-core/register --require ignore-styles --no-deprecation",
    "test:unit-coverage": "nyc report --reporter=lcov coverage/lcov-report && http-server -p 3001 "
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^4.6.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.11.0",
    "nyc": "^13.3.0",
    "react-validation": "^3.0.7",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "transform-runtime": "^0.0.0",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "mocha": "^6.0.2",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "stubby": "^4.0.0",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 80,
    "branches": 80,
    "function": 80,
    "lines": 80,
    "include": [
      "src/actions/*.js",
      "src/app/**/*.js",
      "src/helpers/*.js",
      "src/reducers/*.js"
    ],
    "exclude": [
      "test/**/*.spec.js",
      "src/config/"
    ]
  }
}
