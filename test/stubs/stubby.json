[
  {
    "request": {
      "url": "^/node/api/blog/$",
      "status": 200,
      "method": "GET"
      },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "file": "./response-articles.json"
    }
  },
  {
    "request": {
      "url": "^/node/api/blog/([a-zA-Z0-9]+)",
      "status": 200,
      "method": "GET"
      },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "file": "./articles/content-<% url[1] %>.json"
    }
  },
]
