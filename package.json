{
	"name": "blog",
	"version": "1.1.2",
	"description": "",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "webpack -p --mode production --env.NODE_ENV=production",
		"build:int": "webpack -p --mode development --env.NODE_ENV=integration",
		"start:int": "webpack-dev-server --mode development --env.NODE_ENV=integration",
		"start:dev": "concurrently \"npm run stubby\" \"npm run dev\"",
		"dev": "webpack-dev-server --mode development --env.NODE_ENV=development",
		"start": "npm run start:dev",
		"stubby": "stubby -w -d mocks/stubby.json",
		"lint": "eslint ./src --ignore-path .gitignore --fix",
		"test": "concurrently \"npm run lint\" \"npm run test:unit\"",
		"test:unit": "nyc mocha test/unit --compilers js:@babel/register --require ignore-styles --no-deprecation",
		"test:unit-coverage": "nyc report --reporter=lcov coverage/lcov-report && http-server -p 3001"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Proskynete/blog.git"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"concurrently": "^5.0.0",
		"css-loader": "^3.2.0",
		"eslint": "^6.7.2",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.7.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"file-loader": "^4.3.0",
		"jest": "^24.9.0",
		"jest-fetch-mock": "^2.1.2",
		"jest-localstorage-mock": "^2.4.0",
		"jsdom": "15.2.0",
		"jsdom-global": "3.0.2",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.13.1",
		"prettier": "^1.19.1",
		"sass-loader": "^8.0.0",
		"stubby": "^4.1.1",
		"style-loader": "^0.23.1",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"@babel/core": "^7.7.5",
		"@babel/polyfill": "^7.7.0",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-react": "^7.7.4",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"moment": "^2.24.0",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-helmet": "^6.0.0",
		"react-markdown": "^4.3.1",
		"react-redux": "^7.1.1",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-snap": "^1.23.0",
		"redux": "^4.0.4",
		"redux-thunk": "^2.3.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"jest": {
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": -10
			}
		}
	},
	"reactSnap": {
		"inlineCss": true,
		"puppeteerArgs": [
			"--no-sandbox",
			"--disable-setuid-sandbox"
		]
	}
}
