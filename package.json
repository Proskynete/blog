{
  "name": "blog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --colors",
    "lint": "eslint ./src --ignore-path .gitignore",
    "production": "webpack -p",
    "serve": "webpack-dev-server",
    "start": "concurrently 'yarn stubby' 'yarn serve'",
    "stubby": "stubby -w -d test/stubs/stubby.json",
    "test": "concurrently 'yarn lint' 'yarn test:unit'",
    "test:unit": "nyc mocha test/unit --compilers js:babel-core/register --require ignore-styles --no-deprecation",
    "test:unit-coverage": "nyc report --reporter=lcov coverage/lcov-report && http-server -p 3001 "
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.18.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "http-server": "^0.11.1",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^4.1.0",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^5.0.5",
    "node-sass": "^4.7.2",
    "nyc": "^11.6.0",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.7",
    "stubby": "^4.0.0",
    "style-loader": "^0.20.3",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "html-webpack-plugin": "^3.0.6",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.5.8",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.4",
    "react-validation": "^3.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "transform-runtime": "^0.0.0",
    "webpack": "3"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 80,
    "branches": 80,
    "function": 80,
    "lines": 80,
    "include": [
      "src/actions/*.js",
      "src/app/**/*.js",
      "src/helpers/*.js",
      "src/reducers/*.js"
    ],
    "exclude": [
      "test/**/*.spec.js",
      "src/config/"
    ]
  }
}
